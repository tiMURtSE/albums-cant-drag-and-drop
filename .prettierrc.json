{
	"printWidth": 100,
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"arrowParens": "always",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "never",
	"singleAttributePerLine": true,

	"importOrder": ["^react", "^store", "^hooks", "<THIRD_PARTY_MODULES>", "^assets", "^[./]"]
}
